<?xml version="1.0"?>
<!-- This file is managed by Ansible, any changes made by hand will be overritten. -->
<config control-bind="127.0.0.1:8059" version="1.0">
    <!-- see example-config.xml and config.xsd for more documentation -->
    <web-server>
        <http bind=":8060"{% if fisheye_proxy_name and fisheye_scheme in ["http", "https"] %} proxy-host="{{ fisheye_proxy_name }}"{% if fisheye_scheme == "http" %} proxy-scheme="http" proxy-port="80"{% elif fisheye_scheme == "https" %} proxy-scheme="https" proxy-port="443"{% endif %}{% endif %}/>
    </web-server>
    <security allow-anon="true" allow-cru-anon="true"/>
    <repository-defaults>
        <linker/>
        <allow/>
        <tarball enabled="false" maxFileCount="0"/>
        <security allow-anon="true"/>
    </repository-defaults>
</config>
